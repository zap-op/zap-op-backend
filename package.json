{
  "name": "zap-op-backend",
  "version": "1.0.1",
  "description": "",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "rmdir": "node -e \"var fs = require('fs'); process.argv.slice(1).map((fpath)=>{ try { fs.rmdirSync(fpath, { recursive: true, force: true }); } catch(e) { console.log('Error folder:', fpath, e); } });\"",
    "pre-build":"rmdir src/dist",
    "build": "npm run pre-build ; npx tsc -p src/",
    "start": "npm run build && node --es-module-specifier-resolution=node src/dist/server.js",
    "dev": "npm run build && concurrently \"npx tsc --watch\" \"nodemon -q --es-module-specifier-resolution=node src/dist/server.js\""
  },
  "author": "Huy Pham",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-json-validator-middleware": "^3.0.1",
    "json-schema": "^0.4.0",
    "mongoose": "^6.6.3",
    "mongoose-schema-jsonschema": "^2.0.2",
    "nodemon": "^2.0.20",
    "rxjs": "^7.5.7",
    "uuid": "^9.0.0",
    "validator": "^13.7.0",
    "zaproxy": "^1.0.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/json-schema": "^7.0.11",
    "@types/node": "^18.7.23",
    "@types/nodemon": "^1.19.2",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.7",
    "concurrently": "^7.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}

{
    "name": "zap-op-backend",
    "version": "1.0.1",
    "description": "",
    "type": "module",
    "main": "src/server.js",
    "scripts": {
        "rmdir": "node -e \"var fs = require('fs'); process.argv.slice(-1).map((fpath) => { try { fs.rmSync(fpath, { recursive: true }); } catch (error) { console.warn(`Error: no such file or directory, ulink '${fpath}'`); return; } console.log(`Delete '${fpath}' successfully`); });\"",
        "pre-build": "npm run rmdir dist",
        "build": "npm run pre-build && npx tsc",
        "start": "npm run build && node --es-module-specifier-resolution=node dist/server.js",
        "dev": "npm run pre-build && concurrently \"npx tsc --watch\" \"nodemon -q --es-module-specifier-resolution=node dist/server.js\""
    },
    "author": "Huy Pham",
    "license": "ISC",
    "dependencies": {
        "bson": "^4.7.0",
        "chalk": "^5.1.2",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "express-json-validator-middleware": "^3.0.1",
        "express-jwt": "^7.7.7",
        "google-auth-library": "^8.7.0",
        "json-schema": "^0.4.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^6.7.3",
        "mongoose-schema-jsonschema": "^2.0.2",
        "rxjs": "^7.6.0",
        "validator": "^13.7.0",
        "zaproxy": "^1.0.1"
    },
    "devDependencies": {
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.14",
        "@types/json-schema": "^7.0.11",
        "@types/jsonwebtoken": "^8.5.9",
        "@types/node": "^18.11.9",
        "@types/nodemon": "^1.19.2",
        "@types/validator": "^13.7.10",
        "concurrently": "^7.5.0",
        "dotenv": "^16.0.3",
        "nodemon": "^2.0.20",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4"
    }
}
